<h1>Index</h1>

    <div class="row">
      <br />
      <p><div id="mapholder" class="col-xs-12 col-sm-6 col-md-8 map-canvas"></div></p>
      <div class="col-xs-12 col-md-4">
        <form action="/results" method="GET" role="form">
          <div class="form-group address-input">
            <p><input type="text" class="form-control" name="address1" placeholder="Enter First Address"><p>
            <input type="text" class="form-control" name="address2" placeholder="Enter Second Address">
          </div>
          <button type="submit" class="btn btn-primary pull-right login-button">Submit</button>
        </form>
      </div>
      <div class="col-xs-12 col-md-4 checkbox-area">
        <form class="checkbox-form">
          <br>
          <p><input type="checkbox" name="bakery">&nbsp;&nbsp;Bakeries</p>
          <p><input type="checkbox" name="bank">&nbsp;&nbsp;Banks</p>
          <p><input type="checkbox" name="bar">&nbsp;&nbsp;Bars</p>
          <p><input type="checkbox" name="book_store">&nbsp;&nbsp;Book Stores</p>
          <p><input type="checkbox" name="coffee">&nbsp;&nbsp;Coffee Shops</p>
          <p><input type="checkbox" name="museum">&nbsp;&nbsp;Museums</p>
          <p><input type="checkbox" name="park">&nbsp;&nbsp;Parks</p>
          <p><input type="checkbox" name="restaurant">&nbsp;&nbsp;Restaurants</p>
        </form>
      </div>
    </div>

<script src="https://maps.googleapis.com/maps/api/js?&v=3.21&callback=initMap&libraries=places&key=AIzaSyCjpLWue5GTsHRzJmp4pOxFFJoRGpTMWCc"></script>
